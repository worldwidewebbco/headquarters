{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"jsx": "preserve",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./src/*"]
		},
		"allowJs": true,
		"incremental": true
	},
	"include": ["src", "next.config.ts", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
